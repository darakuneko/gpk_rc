{
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./public/build && rm -rf ./build && rm -rf ./dist && npx webpack",
    "build:win": "del /s /q public\\build && del /s /q build && del /s /q dist && npx webpack",
    "start": "npx webpack && npx electron .",
    "postinstall": "electron-rebuild -f -w node-gyp",
    "dist:win": "npm run build:win && npm run postinstall && electron-builder -w",
    "dist:mac": "npm run build && npm run postinstall && electron-builder -m",
    "dist:linux": "npm run build && npm run postinstall && electron-builder -l"
  },
  "name": "gpk_rc",
  "version": "1.8.4",
  "description": "GPK RC",
  "homepage": "https://github.com/darakuneko",
  "author": {
    "name": "darakuneko",
    "email": "tvkids@gmail.com"
  },
  "resolutions": {
    "nan": "github:tec27/nan#remove_accessor_signature",
    "@inigolabs/ref-napi": "4.0.4"
  },
  "devDependencies": {
    "electron": "23.2.0",
    "electron-builder": "23.6.0",
    "@electron/rebuild": "3.2.10",
    "webpack-cli": "5.0.1"
  },
  "main": "./index.js",
  "dependencies": {
    "@babel/core": "7.21.3",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@emotion/babel-preset-css-prop": "11.10.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@inigolabs/ffi-napi": "4.0.10",
    "@inigolabs/ref-napi": "4.0.6",
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.11.14",
    "active-win": "8.1.0",
    "babel-loader": "9.1.2",
    "dayjs": "1.11.7",
    "electron-store": "8.1.0",
    "find-process": "^1.4.7",
    "fs": "0.0.1-security",
    "iconv": "3.0.1",
    "nan": "github:tec27/nan#remove_accessor_signature",
    "node-hid": "2.1.2",
    "node-loader": "2.0.0",
    "node-window-manager": "2.2.4",
    "path": "0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "9.0.0",
    "webpack": "5.76.3"
  },
  "build": {
    "productName": "GPK RC",
    "appId": "app.darakuneko.gpk_rc",
    "npmRebuild": false,
    "files": [
      "public/**/*",
      "icons/*",
      "index.js",
      "preload.js",
      "qmkrcd.js",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "icons/icon-256x256.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "icons/icon-512x512.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icons"
    }
  }
}
