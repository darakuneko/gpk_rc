{
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./public/build && rm -rf ./build && rm -rf ./dist && npx webpack",
    "build:win": "del /s /q public\\build && del /s /q build && del /s /q dist && npx webpack",
    "start": "npx webpack && npx electron .",
    "dist:win": "npm run build:win && electron-builder -w",
    "dist:mac": "npm run build && electron-builder -m",
    "dist:linux": "npm run build && electron-builder -l"
  },
  "name": "gpk_cr",
  "version": "1.8.0",
  "description": "GPK RC",
  "homepage": "https://github.com/darakuneko",
  "author": {
    "name": "darakuneko",
    "email": "tvkids@gmail.com"
  },
  "devDependencies": {
    "electron": "18.2.4",
    "electron-builder": "23.0.3",
    "electron-rebuild": "3.2.7",
    "webpack-cli": "4.9.2"
  },
  "main": "./index.js",
  "dependencies": {
    "@babel/core": "7.18.0",
    "@babel/preset-env": "7.18.0",
    "@babel/preset-react": "7.17.12",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.8.0",
    "@mui/material": "5.8.1",
    "active-win": "7.7.2",
    "babel-loader": "8.2.5",
    "dayjs": "1.11.2",
    "electron-store": "8.0.1",
    "ffi-napi": "4.0.3",
    "fs": "0.0.1-security",
    "iconv": "3.0.1",
    "node-hid": "2.1.1",
    "node-loader": "2.0.0",
    "node-window-manager": "2.2.4",
    "path": "0.12.7",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "uuid": "8.3.2",
    "webpack": "5.72.1"
  },
  "build": {
    "productName": "GPK RC",
    "appId": "app.darakuneko",
    "files": [
      "public/**/*",
      "icons/*",
      "index.js",
      "preload.js",
      "qmkrcd.js",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "icons/icon-256x256.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "icons/icon-512x512.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icons"
    }
  }
}
